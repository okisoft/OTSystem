<% provide(:title, '進捗一覧画面') %>
<h1>進捗一覧画面</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= link_to "課題編集", '#', class: "btn btn-primary" %>
    <%= link_to "授業終了", '#', class: "btn btn-primary" %>
    <% lecture_time = LectureTime.find_by(id: @public_lectures.first.lecture_time_id) %>
    <%= link_to "質問一覧", questions_lecture_time_path(lecture_time), class: "btn btn-primary" %>
    <% @public_lectures.first.lecture_time_id %>

    <% @achievments_array.each do |achv| %>
      <li>
        <%= achv.first.user.user_id %>
        <%= achv.first.user.name %>

        <%= achv.last %>

        <%= achv.first.icon %>
      </li>
    <% end %>
  </div>
</div>
