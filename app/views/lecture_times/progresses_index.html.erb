<% provide(:title, '進捗一覧画面') %>
<h1>進捗一覧画面</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% @achievments_array.each do |achv| %>
      <li>
        <%= achv.first.user.user_id %>
        <%= achv.first.user.name %>

        <%= achv.last %>

        <%= achv.first.icon %>
      </li>
    <% end %>
  </div>
</div>
