<% provide(:title, '進捗一覧画面') %>
<h1>進捗一覧画面</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% @progresses.each do |progress| %>
      <li>
        <%= progress.user.user_id %>
        <%= progress.user.name %>
        <%= progress.icon %>
        <!--<%= progress.user.name %>-->
      </li>
    <% end %>
  </div>
</div>
